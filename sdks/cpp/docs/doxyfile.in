USE_MDFILE_AS_MAINPAGE = @CMAKE_CURRENT_SOURCE_DIR@/docs/cpp_sdk_main_page.md
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@/doxygen/

PROJECT_LOGO           = @CMAKE_CURRENT_SOURCE_DIR@/../../docs/images/doxylogo.png

